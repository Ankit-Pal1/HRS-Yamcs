{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "webapp": {
      "root": "projects/webapp",
      "sourceRoot": "projects/webapp/src",
      "projectType": "application",
      "schematics": {},
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "projects/webapp/src/index.html",
            "main": "projects/webapp/src/main.ts",
            "tsConfig": "projects/webapp/tsconfig.app.json",
            "polyfills": [
              "zone.js"
            ],
            "preserveSymlinks": true,
            "outputHashing": "bundles",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/webapp/src/assets/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@yamcs/opi/dist/images/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@yamcs/opi/dist/fonts/",
                "output": "/"
              },
              "projects/webapp/src/manifest.webmanifest"
            ],
            "styles": [
              "projects/webapp/src/styles/vars.css",
              "projects/webapp/src/styles/material-theme.scss",
              "projects/webapp/src/styles/material-icons.css",
              "projects/webapp/src/styles/roboto.css",
              "projects/webapp/src/styles/scrollbar.css",
              "projects/webapp/src/styles/styles.css",
              "projects/webapp/src/styles/data-table.css",
              "projects/webapp/src/styles/dl-horizontal.css",
              "projects/webapp/src/styles/form.css"
            ],
            "allowedCommonJsDependencies": [
              "brace",
              "brace/mode/javascript",
              "dayjs",
              "dayjs/locale/en",
              "dayjs/plugin/utc",
              "showdown"
            ],
            "serviceWorker": true,
            "ngswConfigPath": "projects/webapp/ngsw-config.json"
          },
          "configurations": {
            "production": {
              "buildOptimizer": true,
              "optimization": true,
              "vendorChunk": false,
              "extractLicenses": true,
              "sourceMap": false,
              "namedChunks": false
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": false
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
